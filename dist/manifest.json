{
  "name": "Respectful Emoji Tool",
  "short_name": "RET",
  "description": "A must-have tool for any Emojineer",
  "version": "1.0.0",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["*://*.slack.com/*/emoji*"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "images/icon_128.png"
  },
  "manifest_version": 2,
  "web_accessible_resources": ["content.js", "images/icon_128.png"]
}
